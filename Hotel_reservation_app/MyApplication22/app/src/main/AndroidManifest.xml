<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.testnh.myapplication22">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/hotel_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/hotel_icon"
        android:supportsRtl="true"
        android:largeHeap="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JoinActivity" android:screenOrientation="portrait"/>

        <activity android:name=".Join2Activity" android:screenOrientation="portrait"/>

        <activity android:name=".Join3Activity" android:screenOrientation="portrait"/>

        <activity android:name=".LonInActivity" android:screenOrientation="portrait"/>

        <activity android:name=".RoomActivity" android:screenOrientation="portrait"/>

        <activity android:name=".ShowActivity" android:screenOrientation="portrait"/>

        <activity android:name=".CheckActivity" android:screenOrientation="portrait"/>

        <activity android:name=".listView" android:screenOrientation="portrait"/>

        <activity android:name=".Write" android:screenOrientation="portrait"/>

        <activity android:name=".Detail" android:screenOrientation="portrait"/>
    </application>

</manifest>